# Flutter/Dart general ignores
*.dart_tool/
.dart_tool/
.packages
pubspec.lock  # Optional: Include this if you want reproducible builds across team
build/
*.g.dart  # Generated Dart files (e.g., from build_runner)
*.mocks.dart  # Mock files generated by mockito or similar tools

# Dependency directories
.dart_dependencies/
.packages.generated

# Logs and temporary files
*.log
*.log.*
*.tmp
*.temp
*.swp

# IDE/Editor files
.idea/  # IntelliJ/Android Studio
*.iml   # IntelliJ module files
.vscode/  # Visual Studio Code (optional: keep if sharing settings)
*.sublime-*  # Sublime Text files
*.code-workspace

# Flutter specific
flutter_*.log
flutter_export_environment.sh
flutter_build_*.log
*.flutter-plugins
*.flutter-plugins-dependencies

# Generated files by Flutter
lib/generated_plugin_registrant.dart
ios/Flutter/Generated.xcconfig
ios/Flutter/AppFrameworkInfo.plist

# macOS-specific
.DS_Store

# Android-specific
android/.gradle/
android/build/
android/app/build/
android/local.properties  # Contains SDK path, should not be committed
android/*.keystore  # Keystore files for signing
android/*.jks       # Java Keystore files
android/key.properties  # Contains keystore passwords (security-sensitive)

# iOS-specific
ios/.build/
ios/Pods/
ios/Podfile.lock
ios/Runner.xcworkspace/xcshareddata/
ios/Runner.xcworkspace/xcuserdata/
ios/Flutter/Flutter-Generated.xcconfig
ios/*.pbxproj.backup
ios/*.mode1v3
ios/*.mode2v3
ios/*.perspectivev3

# Firebase-specific
# Ignore Firebase config files with sensitive data
android/app/src/*/google-services.json  # Ignore in flavor dirs (dev, prod, etc.)
ios/config/*/GoogleService-Info.plist   # Ignore in flavor-specific config dirs
firebase.json  # Firebase CLI config file (if present)
.firebaserc    # Firebase project aliases

# Security-sensitive files
# API keys, credentials, and secrets
*.env           # Environment variable files
*.key           # Generic key files
*.pem           # Certificate files
*.cert          # Certificates
*.p12           # PKCS12 files
secrets.json    # Common secret file name
credentials.json  # Common credential file name

# Backup and miscellaneous
*.bak
*.backup
*.old
*.orig

# Coverage and test reports
coverage/
lcov.info
test/*.xml
test/*.json

# Miscellaneous
*.zip
*.tar.gz
*.tgz
*.rar